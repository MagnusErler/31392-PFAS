<div class="line"><a name="l00160"></a><span class="lineno">  160</span>&#160;    <a class="code" href="classopen3d_1_1core_1_1_device.html">core::Device</a> device(params.<a class="code" href="structopen3d_1_1t_1_1pipelines_1_1slac_1_1_s#abe8c47fea486839deb440bb40ffe0e40">device_</a>);</div>
<div class="line"><a name="l00161"></a><span class="lineno">  161</span>&#160;    <span class="keywordtype">int</span> n_frags = pose_graph.<a class="code" href="classopen3d_1_1pipelines_1_1registration_1_1-2#a74e6d36b70797a291ff519585dcf2357">nodes_</a>.size();</div>
<div class="line"><a name="l00162"></a><span class="lineno">  162</span>&#160; </div>
<div class="line"><a name="l00163"></a><span class="lineno">  163</span>&#160;    <span class="comment">// Enumerate pose graph edges.</span></div>
<div class="line"><a name="l00164"></a><span class="lineno">  164</span>&#160;    <span class="keywordflow">for</span> (<span class="keyword">auto</span>&amp; edge : pose_graph.<a class="code" href="classopen3d_1_1pipelines_1_1registration_1_1-2#aef356fe8a1c8d47d3807ef710a426098">edges_</a>) {</div>
